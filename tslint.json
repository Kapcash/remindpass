{
  "rules": {
    "no-duplicate-variable": true,
    "no-unused-variable": [
      true,
      {
        "ignore-pattern": "^_"
      }
    ],
    "no-empty-interface": true,
    "no-magic-numbers": true,
    "only-arrow-functions": true,
    "ban-comma-operator": true,
    "no-debugger": true,
    "no-duplicate-imports": true,
    "no-duplicate-super": true,
    "no-eval": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "max-line-length": [
      true,
      140
    ],
    "max-file-line-count": [
      true,
      450
    ],
    "prefer-const": true,
    "trailing-comma": true,
    "array-type": [
      true,
      "generic"
    ],
    "arrow-parens": true,
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "jsdoc-format": true,
    "no-trailing-whitespace": true,
    "number-literal-format": true,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": true,
    "space-before-function-paren": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-type",
      "check-preblock"
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
